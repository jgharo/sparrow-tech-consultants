<p id="notice"><%= notice %></p>

<h1>Prod Statuses</h1>

<table>
  <thead>
    <tr>
      <th>Prod status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @prod_statuses.each do |prod_status| %>
      <tr>
        <td><%= prod_status.prod_status %></td>
        <td><%= link_to 'Show', prod_status %></td>
        <td><%= link_to 'Edit', edit_prod_status_path(prod_status) %></td>
        <td><%= link_to 'Destroy', prod_status, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Prod Status', new_prod_status_path %>
