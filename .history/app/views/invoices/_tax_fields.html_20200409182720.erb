<div class='nested-fields'>

  <div class="field">
    <%= f.label :service_id, "Service:" %>
    <%= f.collection_select :stlservice_id, Stlservice.all, :id, :service_name, :required => true %>
  </div>

  <div class="field">
    <%= f.label :tax_id, "Tax Rate:" %>
    <%= f.collection_select :tax_id, Tax.all, :id, :rate, :required => true%>
  </div>

  <%= link_to_remove_association "Remove Service", f%>
</div>